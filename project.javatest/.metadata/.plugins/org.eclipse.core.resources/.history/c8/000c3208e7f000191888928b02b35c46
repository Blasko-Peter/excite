package com.xcite.javatest.action.web;

import java.io.IOException;

import javax.servlet.ServletException;
import javax.servlet.annotation.WebServlet;
import javax.servlet.http.HttpServlet;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

@WebServlet(name = "Edit", urlPatterns = { "/Edit" })
public class Edit extends HttpServlet {

	private static final long serialVersionUID = 1L;

	public void doPost(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {
		String loginFormEmail = request.getParameter("email");
		System.out.println(loginFormEmail);
		String loginFormPassword = request.getParameter("password");
		System.out.println(loginFormPassword);
	}

}
